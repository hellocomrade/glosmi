package us.glos.mi.domain;

public class ReferenceParam {
	private String categoryItem=null;
	private String themeItem=null;
	public static String getClassName()
	{
		return ReferenceParam.class.getName();
	}
	public String getCategoryItem() {
		return categoryItem;
	}
	public void setCategoryItem(String categoryItem) {
		this.categoryItem = categoryItem;
	}
	public String getThemeItem() {
		return themeItem;
	}
	public void setThemeItem(String themeItem) {
		this.themeItem = themeItem;
	}
	public String getCategoryParamName()
	{
		return "categoryitem";
	}
	public String getThemeParamName()
	{
		return "themeitem";
	}
}
