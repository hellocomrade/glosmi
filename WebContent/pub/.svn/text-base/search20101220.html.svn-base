<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Great Lakes Model Inventory Search Page</title>
<link href="pub.css" rel="stylesheet" type="text/css" />
<style>
body{
	background:url("../img/fisheye/main-bg.gif") repeat scroll 0 0 #FFFFFF;
}
</style>
<script type="text/javascript">
function keycheck()
{
	var id=document.getElementById('txt_sbox');
	if(id!=null)
	{
		if(id.value!='')return true;
		else return false;
	}
	return false;
}
</script>
</head>
<body>
<div align="center" style="margin-top:200px;">
	<table id="links" width="50%" cellpadding="0" bgcolor="#CEE22A;" class="loginform">
						<tbody>
							<tr style="background-color:#CEE22A;">
								<td valign="top">
									<div align="center" style="margin:10px 0pt;">
										<!--  <span class="loginfont" style="font-weight:bold;">Search GLOS Model Inventory</span>-->
										<img src="../img/glmibanner.png" alt="GLOS Model Inventory" />
									</div>
									<div align="center" style="margin:6px 10px 0 10px;padding-bottom:16px;">
									    <form name="f" action="search.glos" method="GET" onsubmit="return keycheck();">
											<table cellspacing="0" cellpadding="0" border="0" style="border-bottom:1px solid #e7e7e7;padding:8px 0pt 0pt;">
												<tr>
													<td class="searchbox-td" style="width:100%;">
														<input type="text" id="txt_sbox" value="" maxlength="255" size="40" name="key" class="searchbox" style="background-color:#FFFFA2;"/>
														<input type="hidden" value="10" name="l"/>
														<input type="hidden" value="0" name="o"/>
														<input type="hidden" value="0" name="f"/>
														<input type="hidden" value="" name="of"/>
													</td>
													<td>
														<div class="searchbtn-outter">
															<div class="searchbtn-inner">
																<input class="searchbtn" type="submit" value="Search" />
															</div>
														</div>
													</td>
												</tr>
											</table>
										</form>
									</div>
									
								</td>
							</tr>
						</tbody>
					</table>
					
</div>
</body>
</html>